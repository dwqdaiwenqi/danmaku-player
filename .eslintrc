{
	"extends": "standard",
	"rules": {
		"one-var": 0,
		"no-mixed-operators": 0,
		"standard/no-callback-literal": 0,
		"new-cap": 0,
		"no-tabs": 0,
		"camelcase": 0,
		"no-lone-blocks": 0,
		"no-cond-assign": 0,
		"no-return-assign": 0,
		"no-mixed-spaces-and-tabs": 0,
		"no-floating-decimal": 0,
		"indent": ["off", "tab"]
	},
	"parser": "babel-eslint"
}